type Query {
    roles: [Role!]!
}

type Mutation {
    userSave(username: String!, password: String!): User
}

type User {
    id: ID
    publicName: String!
    firstName: String
    lastName: String
    email: String!
    emailVerifiedAt: DateTime
    password: String
    roles: [Role!]!
}
#
type Role {
    id: ID
    name: String!
}